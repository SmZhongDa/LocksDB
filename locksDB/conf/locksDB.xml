<conf>
	<locksDB>
		<!-- bcp文件目录 -->
		<dataSource>####</dataSource>
		<!-- 数据文件目录 -->
		<dataPath>/data/rocksdb</dataPath>
		<!-- 数据文件日志目录 -->
		<WALPath>/data/wal</WALPath>
		<!-- 索引文件目录 -->
		<indexPath>/data/lucene</indexPath>
		<!-- 平台库连接串 -->
		<url>jdbc:oracle:thin:@####</url>
		<!-- 平台库用户名 -->
		<user>####</user>
		<!-- 平台库密码 -->
		<passwd>####</passwd>
		<!-- 数据保存天数 -->
		<interval>####</interval>
		<!-- 是否开启维度统计功能 1:开启 0:关闭 -->
		<dimension>####</dimension>
	</locksDB>
	<core>
		<period>60</period>
		<sleep>60</sleep>
		<release>60</release>
		<reloader>60</reloader>
		<taxoRelease>60</taxoRelease>
		<taxoReloader>60</taxoReloader>
		<port>23456</port>
	</core>
	<dimension>
		<!-- 字段名:bcp字段位置,从0开始:子维度位置偏移:多值域分隔符 -->
		<field>USERTYPE:1:4:,LOCATION:2::,APPLICATION:3:6:,BEHAVIOR:6:7:#,AUTHTYPE:7::,PERIOD:9::,TERMINALID:14::#,NBKEYWORD:12:5:#,NBDATASOURCE:10:6:,NBSTORE:17::#
		</field>
	</dimension>
	<protocol>
	    <!-- 协议名:协议编号 -->
	    <protocoltoid>AUTH:101,HTTP:102,IM:103,EMAIL:104,EXPRESS:105,GOODS:106,HOTEL:107,FLIGHT:108,RESUME:109,GAMBLE:110,TRACK:111,OVERNET:112,MBLOG:113,SNS:114,FTP:115,MMS:116,OTHER:117,BLOG:118,WEBBBS:119,P2P:120,STREAMMEDIA:121,UPLOAD:122,RECHARGE:123,RENT:124,TRAIN:125,BUS:126,LOCATION:127,MASSPHONEBOOK:128,GAME:129,TELNET:130,VOIP:131,PROXY:132,VPN:133,REMOTECTRL:134,WCALL:135,CCALL:136,WVSMS:137,CVSMS:138,WEVET:139,CNETP:142,DEVICEGSM:143,DEVICEWIFI:144,DEVICEAPP:145,DEVICEDATA:146,TERMINALINFO:147,DNS:148,ENGINE:149,TAXI:150,WEBSHARE:151,NEWS:152,SMS:153,WEBCHAT:154,NETBANK:155,PAYMENT:156,MAP:157,VIDEO:158,VOICE:159,APPSTORE:160,MOBILEKEEPER:161,HTTPS:162,COMMUNICATION:163,MARRY:164,INPUTMETHOD:165,VPEOPLEMOVEMENT:166,PICTURE:167,NAVIGATION:168,MULTIMEDIAIMAGE:169
	    </protocoltoid>
	    <idtoprotocol>101:AUTH,102:HTTP,103:IM,104:EMAIL,105:EXPRESS,106:GOODS,107:HOTEL,108:FLIGHT,109:RESUME,110:GAMBLE,111:TRACK,112:OVERNET,113:MBLOG,114:SNS,115:FTP,116:MMS,117:OTHER,118:BLOG,119:WEBBBS,120:P2P,121:STREAMMEDIA,122:UPLOAD,123:RECHARGE,124:RENT,125:TRAIN,126:BUS,127:LOCATION,128:MASSPHONEBOOK,129:GAME,130:TELNET,131:VOIP,132:PROXY,133:VPN,134:REMOTECTRL,135:WCALL,136:CCALL,137:WVSMS,138:CVSMS,139:WEVET,142:CNETP,143:DEVICEGSM,144:DEVICEWIFI,145:DEVICEAPP,146:DEVICEDATA,147:TERMINALINFO,148:DNS,149:ENGINE,150:TAXI,151:WEBSHARE,152:NEWS,153:SMS,154:WEBCHAT,155:NETBANK,156:PAYMENT,157:MAP,158:VIDEO,159:VOICE,160:APPSTORE,161:MOBILEKEEPER,162:HTTPS,163:COMMUNICATION,164:MARRY,165:INPUTMETHOD,166:VPEOPLEMOVEMENT,167:PICTURE,168:NAVIGATION,169:MULTIMEDIAIMAGE
